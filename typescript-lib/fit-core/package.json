{
  "name": "fit-core",
  "version": "0.0.1",
  "description": "Fittable core",
  "author": "@costar",
  "license": "ISC",
  "scripts": {
    "clean": "rm -r ./dist && mkdir ./dist",
    "build": "tsc -p tsconfig.json && cp ./package.json ./dist",
    "watch": "tsc -p tsconfig.json --watch",
    "test": "cross-env NODE_OPTIONS=--loader=ts-node/esm jasmine --config=jasmine.json",
    "test:coverage": "nyc --show-process-tree npm test"
  },
  "type": "module",
  "devDependencies": {
    "@types/jasmine": "^4.3.0",
    "@types/node": "^17.0.25",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "cross-env": "^7.0.3",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "rxjs": "^7.8.0"
  }
}