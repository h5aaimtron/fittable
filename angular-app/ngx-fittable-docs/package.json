{
  "name": "ngx-fittable-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch",
    "reset": "rm -r dist & rm -r node_modules & rm package-lock.json & npm cache clean --force",
    "link": "npm link fittable-core fittable-model fittable-model-operations fittable-view-model fittable-angular",
    "clean": "rm -r ./dist & rm -r -f ./.angular/cache",
    "build": "ng build  && cp ./LICENSE ./dist/ngx-fittable-docs",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npx eslint ./src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.3",
    "@angular/common": "^16.0.3",
    "@angular/compiler": "^16.0.3",
    "@angular/core": "^16.0.3",
    "@angular/forms": "^16.0.3",
    "@angular/platform-browser": "^16.0.3",
    "@angular/platform-browser-dynamic": "^16.0.3",
    "@angular/router": "^16.0.3",
    "fittable-angular": "^3.1.0",
    "npm": "^9.6.6",
    "primeicons": "^6.0.1",
    "primeng": "^16.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/cli": "~16.0.2",
    "@angular/compiler-cli": "^16.0.3",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.2"
  }
}
